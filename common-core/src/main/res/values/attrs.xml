<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="Rotate3dAnimation">
        <attr name="rollType" format="enum">
            <enum name="x" value="0"/>
            <enum name="y" value="1"/>
            <enum name="z" value="2"/>
        </attr>
        <attr name="fromDeg" format="float" />
        <attr name="toDeg" format="float" />
        <attr name="pivotX" format="fraction"/>
        <attr name="pivotY" format="fraction" />
    </declare-styleable>


    <declare-styleable name="ChangeableTextView">
        <attr name="openAnim" format="boolean"></attr>
        <attr name="minLines" format="integer"/>
        <attr name="animBackgroundColor" format="color"/>
        <attr name="duration" format="integer"/>
        <attr name="maskword" format="string"/>
        <attr name="replacetext" format="string"/>
    </declare-styleable>
</resources>